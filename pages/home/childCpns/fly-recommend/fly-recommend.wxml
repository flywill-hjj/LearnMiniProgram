<!--pages/home/childCpns/fly-recommend/fly-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{ recommends }}" wx:key="index">
    <view class="recommend-item">
      <image src="{{ item.image }}" class="image" bindload="handleImageLoad" />
      <view class="view">{{ item.title }}</view>
    </view>
  </block>
</view>
